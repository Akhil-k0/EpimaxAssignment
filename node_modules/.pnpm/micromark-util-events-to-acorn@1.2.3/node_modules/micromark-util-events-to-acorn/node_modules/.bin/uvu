#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/classroom-assignment/node_modules/.pnpm/uvu@0.5.6/node_modules/uvu/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/uvu@0.5.6/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/node_modules:/home/theia/classroom-assignment/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/micromark-util-events-to-acorn@1.2.3/node_modules/micromark-util-events-to-acorn/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/micromark-util-events-to-acorn@1.2.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/classroom-assignment/node_modules/.pnpm/uvu@0.5.6/node_modules/uvu/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/uvu@0.5.6/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/node_modules:/home/theia/classroom-assignment/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/micromark-util-events-to-acorn@1.2.3/node_modules/micromark-util-events-to-acorn/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/micromark-util-events-to-acorn@1.2.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../uvu@0.5.6/node_modules/uvu/bin.js" "$@"
else
  exec node  "$basedir/../../../../../uvu@0.5.6/node_modules/uvu/bin.js" "$@"
fi
