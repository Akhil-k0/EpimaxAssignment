#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/classroom-assignment/node_modules/.pnpm/esbuild@0.16.3/node_modules/esbuild/bin/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/esbuild@0.16.3/node_modules/esbuild/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/esbuild@0.16.3/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/node_modules:/home/theia/classroom-assignment/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@esbuild-plugins+node-modules-polyfill@0.1.4_esbuild@0.16.3/node_modules/@esbuild-plugins/node-modules-polyfill/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@esbuild-plugins+node-modules-polyfill@0.1.4_esbuild@0.16.3/node_modules/@esbuild-plugins/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@esbuild-plugins+node-modules-polyfill@0.1.4_esbuild@0.16.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/classroom-assignment/node_modules/.pnpm/esbuild@0.16.3/node_modules/esbuild/bin/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/esbuild@0.16.3/node_modules/esbuild/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/esbuild@0.16.3/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/node_modules:/home/theia/classroom-assignment/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@esbuild-plugins+node-modules-polyfill@0.1.4_esbuild@0.16.3/node_modules/@esbuild-plugins/node-modules-polyfill/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@esbuild-plugins+node-modules-polyfill@0.1.4_esbuild@0.16.3/node_modules/@esbuild-plugins/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@esbuild-plugins+node-modules-polyfill@0.1.4_esbuild@0.16.3/node_modules"
fi
"$basedir/../../../../../../esbuild@0.16.3/node_modules/esbuild/bin/esbuild"   "$@"
exit $?
