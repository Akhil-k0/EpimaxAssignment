#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/classroom-assignment/node_modules/.pnpm/@vercel+remix-run-dev@1.15.0/node_modules/@vercel/remix-run-dev/dist/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@vercel+remix-run-dev@1.15.0/node_modules/@vercel/remix-run-dev/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@vercel+remix-run-dev@1.15.0/node_modules/@vercel/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@vercel+remix-run-dev@1.15.0/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/node_modules:/home/theia/classroom-assignment/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@vercel+remix-builder@1.8.5/node_modules/@vercel/remix-builder/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@vercel+remix-builder@1.8.5/node_modules/@vercel/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@vercel+remix-builder@1.8.5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/classroom-assignment/node_modules/.pnpm/@vercel+remix-run-dev@1.15.0/node_modules/@vercel/remix-run-dev/dist/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@vercel+remix-run-dev@1.15.0/node_modules/@vercel/remix-run-dev/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@vercel+remix-run-dev@1.15.0/node_modules/@vercel/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@vercel+remix-run-dev@1.15.0/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/node_modules:/home/theia/classroom-assignment/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@vercel+remix-builder@1.8.5/node_modules/@vercel/remix-builder/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@vercel+remix-builder@1.8.5/node_modules/@vercel/node_modules:/home/theia/classroom-assignment/node_modules/.pnpm/@vercel+remix-builder@1.8.5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@vercel+remix-run-dev@1.15.0/node_modules/@vercel/remix-run-dev/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../@vercel+remix-run-dev@1.15.0/node_modules/@vercel/remix-run-dev/dist/cli.js" "$@"
fi
